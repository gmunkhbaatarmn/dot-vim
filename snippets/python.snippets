snippet env
	#!/usr/bin/env python
	# -*- coding: utf-8 -*-
snippet utf
	# coding: utf-8
snippet __main__
	if __name__ == "__main__":
	    ${1:main()}
snippet from
	from ${1} import ${2}
snippet imp
	import ${1}
snippet frein
	sys.stdin = open("input.txt")
snippet freout
	sys.stdout = open("output.txt", "w+")
snippet contest
	import sys

	sys.setrecursionlimit(1 << 30)

	for line in sys.stdin:
	    line
